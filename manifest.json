{
    "name": "getAllLink",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "get all links in the page",
    "icons": {
        "16": "icon/icon_16.png",
        "32": "icon/icon_32.png",
        "48": "icon/icon_48.png",
        "128": "icon/icon_128.png"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": [
            "js/jquery-3.5.1.min.js",
            "js/getAllLink.js"
        ]
    }],
    "background": {
        "scripts": ["js/background.js"]
    },
    "browser_action": {
        "default_icon": "icon/icon_32.png",
        "default_title": "getAllLink"
    },
    "permissions": [
		"tabs",
		"background",
		"http://*/*",
		"https://*/*"
	]
}