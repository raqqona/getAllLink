{
    "name": "getAllLink",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "get all links in the page",
    "content_scripts": [
        {
          "matches": [ "http://*/*", "https://*/*" ],
          "js": [
              "jquery-3.5.1.min.js",
              "getLinks.js"
          ]
        }
    ],
    "browser_action": {
        "default_icon": "icon/icon_32.png",
        "default_title": "getAllLink",
        "default_popup": "popup.html"
    },
    "permissions": ["tabs"]
}